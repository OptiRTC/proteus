{
	"extends": "../proteusconfig",
	"compilerOptions": {
		"allowJs": true,
		"baseUrl": "./",
		"paths" : {
			"*" : [
				"src/*",
				"node_modules/*",
				"@types/*"
			],
			"common:*" : [
				"../common/src/*"
            ],
            "worker:*" : [
                "src/*"
            ]
		},
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true
	},
	"references": [
        { "path": "../common" }
	],
	"include": [
		"src"
	]
}