{
	"extends": "../proteusconfig",
	"compilerOptions": {
		"baseUrl": "./",
		"paths" : {
			"*" : [
				"src/*",
				"node_modules/*",
				"@types/*"
			],
			"common/*" : [
				"../common/src/*"
            ],
            "worker/*" : [
                "src/*"
            ]
		}
	},
    "include": [
		"./src/*"
	],
	"references": [
        { "path": "../common" }
	]
}