{
	"extends": "../proteusconfig",
	"compilerOptions": {
		"composite": true,
		"baseUrl": "./",
		"paths" : {
			"*" : [
				"src/*",
				"node_modules/*",
				"@types/*"
			],
			"common/*" : [
				"../common/src/*"
			],
			"core/*" : [
				"src/*"
			] 
		}
	},
    "include": [
		"./src/*"
	],
	"references": [
		{ "path": "../common" }
	]
}
